<p id="notice"><%= notice %></p>

<h1>Literals</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @literals.each do |literal| %>
      <tr>
        <td><%= link_to 'Show', literal %></td>
        <td><%= link_to 'Edit', edit_literal_path(literal) %></td>
        <td><%= link_to 'Destroy', literal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Literal', new_literal_path %>
